{
  "name": "mvp-api",
  "version": "1.0.0",
  "description": "MVP API with async indexing",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/server.js",
    "dev": "ts-node-dev --respawn --transpile-only --poll src/server.ts",
    "seed": "ts-node src/scripts/seed.ts",
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:coverage": "jest test/unit --coverage",
    "test:coverage:ci": "jest test/unit --coverage --coverageReporters=text --coverageReporters=lcov --coverageReporters=json-summary",
    "test:integration": "jest test/integration --runInBand",
    "test:db": "jest test/integration/database.integration.test.ts --runInBand",
    "test:file": "jest test/integration/file-metadata.integration.test.ts --runInBand",
    "test:watch": "jest --watch",
    "test:e2e": "jest -c jest.e2e.config.js --runInBand",
    "test:e2e:basic": "jest -c jest.e2e.config.js --runInBand --testPathPattern=01-basic",
    "test:e2e:fast": "jest -c jest.e2e.config.js --runInBand --testPathPattern='(01-basic|02-cache|06-production)'",
    "test:e2e:quality": "jest -c jest.e2e.config.js --runInBand --testPathPattern='(07-search-quality|08-hybrid-rerank|09-search-quality-rerank)'",
    "test:e2e:large": "jest -c jest.e2e.config.js --runInBand --testPathPattern='(04-large-data|05-scale)'",
    "lint": "eslint src test --ext .ts",
    "lint:fix": "eslint src test --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "clean": "../clean.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@bull-board/api": "^6.14.2",
    "@bull-board/express": "^6.14.2",
    "@bull-board/ui": "^6.14.2",
    "@qdrant/js-client-rest": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "@google/generative-ai": "^0.21.0",
    "openai": "^6.9.1",
    "pdf-parse": "^1.1.1",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "axios": "^1.6.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "form-data": "^4.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
