{
  "name": "@rag/text-utils",
  "version": "1.0.0",
  "description": "Text processing utilities for RAG system - chunking, extraction, normalization",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@langchain/textsplitters": "^1.0.1",
    "langchain": "^1.2.3",
    "pdf-parse": "^1.1.1",
    "pdf.js-extract": "^0.2.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/pdf-parse": "^1.1.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.6.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
